@model AWSUploadFile.Domain.uploadFormObject

@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Upload</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">

</head>
<body>


    <div class="container">

        <div class="col-xl-5 col-lg-6 col-md-8 col-sm-10 mx-auto text-center form p-4">

            @using (Html.BeginForm("UploadFile", "Upload", FormMethod.Post, new { @class = "justify-content-center", enctype = "multipart/form-data" }))
            {
<div class="form-group">
    @Html.TextBox("file", "", new { type = "file" }) <br />

</div>

                <div class="form-group">
                    <label>Email</label>
                    <input type="text" name="Email" id="Email" class="form-control" placeholder="test@email.com">
                </div>
                                <button type="submit" class="btn btn-primary btn-lg">Upload</button>}
        </div>

    </div>


</body>
</html>